<p id="notice"><%= notice %></p>

<h1>Contactos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre contact</th>
      <th>Apellido contact</th>
      <th>Email contact</th>
      <th>Telef contact</th>
      <th>Consulta contact</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactos.each do |contacto| %>
      <tr>
        <td><%= contacto.nombre_contact %></td>
        <td><%= contacto.apellido_contact %></td>
        <td><%= contacto.email_contact %></td>
        <td><%= contacto.telef_contact %></td>
        <td><%= contacto.consulta_contact %></td>
        <td><%= link_to 'Show', contacto %></td>
        <td><%= link_to 'Edit', edit_contacto_path(contacto) %></td>
        <td><%= link_to 'Destroy', contacto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contacto', new_contacto_path %>
